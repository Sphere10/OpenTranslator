@{
	ViewBag.Title = "Open Translation";

}
<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
			<div class="bar">
				<br /><br />
				@(Html.Awe().Grid("MissingTranslation")
						.Mod(o => o.ColumnsSelector().PageSize().PageInfo().Loading().AutoMiniPager())
						.Url(Url.Action("GetAllMissingTranslations", "MissingTranslation"))
						.Groupable(false)
						.Columns(
						new Column { Bind = "LanguageName" , Header = "Language Name" },
						new Column { Bind = "MissingTranslationCount" , Header = "Missing Translation Count" }))
				<br /><br />


			</div>
		</div>
	</div>
	</div>
	<script>

		function logout() {
			var url = '@Url.Action("Index", "User")';
			window.location = url;
		}


	</script>
